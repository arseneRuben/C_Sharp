<Window x:Class="ExamVIEW.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamVIEW"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="66" x:Name="name" Margin="383,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="258" Background="#FFF0E5E5"/>
        <PasswordBox HorizontalAlignment="Left" Margin="383,193,0,0"  x:Name="password"  VerticalAlignment="Top" Height="63" Width="258" Background="#FFECE5E5" Password=""/>
        <Label Content="Name"  HorizontalAlignment="Left" Margin="203,109,0,0" VerticalAlignment="Top" Height="28" Width="92"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="203,193,0,0" VerticalAlignment="Top" Height="28" Width="92"/>
        <Button Content="LOGIN" HorizontalAlignment="Left" Margin="310,335,0,0" VerticalAlignment="Top" Width="173" Height="46" Click="Button_Click"/>
    </Grid>
</Window>
